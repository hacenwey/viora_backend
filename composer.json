{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|8.0",
        "artem-schander/l5-modular": "^2.1",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "beyondcode/laravel-websockets": "^1.9",
        "doctrine/dbal": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "haruncpi/laravel-id-generator": "^1.0",
        "joedixon/laravel-translation": "^1.1",
        "laravel-notification-channels/twilio": "^3.1",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/sanctum": "^2.8",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "~1.0.29",
        "maatwebsite/excel": "^3.1",
        "matt-daneshvar/laravel-survey": "^0.1.3",
        "milon/barcode": "^8.0",
        "mpdf/mpdf": "^8.1",
        "nuovo/spreadsheet-reader": "^0.5.11",
        "propaganistas/laravel-phone": "^4.3",
        "pusher/pusher-php-server": "^4.1",
        "qcod/laravel-settings": "^1.0",
        "shipu/themevel": "^2.2",
        "spatie/laravel-searchable": "^1.9",
        "spatie/laravel-sluggable": "^2.6",
        "srmklive/paypal": "~1.0",
        "symfony/intl": "^5.2",
        "twilio/sdk": "^6.24",
        "typicms/nestablecollection": "^1.1",
        "unisharp/laravel-filemanager": "^2.1",
        "yajra/laravel-datatables-oracle": "^9.11"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files":[
            "app/Http/Helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
